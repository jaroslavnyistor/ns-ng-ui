{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nikisoft-ng-ui": {
      "projectType": "library",
      "root": "projects/nikisoft-ng-ui",
      "sourceRoot": "projects/nikisoft-ng-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/nikisoft-ng-ui/tsconfig.lib.json",
            "project": "projects/nikisoft-ng-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nikisoft-ng-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nikisoft-ng-ui/src/test.ts",
            "tsConfig": "projects/nikisoft-ng-ui/tsconfig.spec.json",
            "karmaConfig": "projects/nikisoft-ng-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nikisoft-ng-ui/tsconfig.lib.json",
              "projects/nikisoft-ng-ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "nikisoft-ng-ui-playground": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "projects/nikisoft-ng-ui-playground",
      "sourceRoot": "projects/nikisoft-ng-ui-playground/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/nikisoft-ng-ui-playground",
            "index": "projects/nikisoft-ng-ui-playground/src/index.html",
            "main": "projects/nikisoft-ng-ui-playground/src/main.ts",
            "polyfills": "projects/nikisoft-ng-ui-playground/src/polyfills.ts",
            "tsConfig": "projects/nikisoft-ng-ui-playground/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/nikisoft-ng-ui-playground/src/favicon.ico",
              "projects/nikisoft-ng-ui-playground/src/assets",
              "projects/nikisoft-ng-ui-playground/src/assets/favicon"
            ],
            "styles": [
              "projects/nikisoft-ng-ui-playground/src/styles.sass"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/nikisoft-ng-ui-playground/src/environments/environment.ts",
                  "with": "projects/nikisoft-ng-ui-playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nikisoft-ng-ui-playground:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "nikisoft-ng-ui-playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nikisoft-ng-ui-playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nikisoft-ng-ui-playground/src/test.ts",
            "polyfills": "projects/nikisoft-ng-ui-playground/src/polyfills.ts",
            "tsConfig": "projects/nikisoft-ng-ui-playground/tsconfig.spec.json",
            "karmaConfig": "projects/nikisoft-ng-ui-playground/karma.conf.js",
            "assets": [
              "projects/nikisoft-ng-ui-playground/src/favicon.ico",
              "projects/nikisoft-ng-ui-playground/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/nikisoft-ng-ui-playground/src/styles.sass"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nikisoft-ng-ui-playground/tsconfig.app.json",
              "projects/nikisoft-ng-ui-playground/tsconfig.spec.json",
              "projects/nikisoft-ng-ui-playground/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/nikisoft-ng-ui-playground/e2e/protractor.conf.js",
            "devServerTarget": "nikisoft-ng-ui-playground:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nikisoft-ng-ui-playground:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "099c0516-8f0b-4e8a-918d-7f98284bc7f2"
  },
  "defaultProject": "nikisoft-ng-ui-playground"
}
